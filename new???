    <svg id="barcode"></svg>
    <script>
        JsBarcode("#barcode", "01000029275371171910200117", {
            format: "CODE128",
            displayValue: true,
            lineColor: "#000",
            width: 2,
            height: 100,
            textMargin: 0,
            fontSize: 16,
            background: "#ffffff"
        });
    </script>
</body>
